#include <iostream>

using namespace std;

int main()
{
    int x;
    cout<<"enter the number: ";
    cin>>x;
    if(x<0){
        cout<<"Invalid number";
    }
    else{
        for(int i=0;i<=x;i++)
        if(i%2==0)
        {
            cout<<i<<" ";
            i++;
        }
    }
    return 0;
}
